{
  "author": "stuyk",
  "type": "module",
  "scripts": {
    "clean": "shx rm -rf resources/core",
    "dev": "nodemon -e ts -e vue -w ./src -w ./webview -x pnpm start",
    "build": "pnpm clean && pnpm transpile && shx cp ./src/resource.toml ./resources/core/resource.toml && pnpm webview:build && pnpm script:imports",
    "start": "pnpm script:kill && pnpm build && pnpm script:paths && altv-server",
    "setup": "altv-pkg i",
    "[-] Scripts": "",
    "script:kill": "pnpm tsx ./scripts/pkill.ts",
    "script:imports": "pnpm tsx ./scripts/buildPluginImports.ts",
    "script:paths": "pnpm tsx ./scripts/pathResolver.ts",
    "[-] Transpile": "",
    "transpile": "sucrase ./src -d ./resources/core --transforms typescript -q",
    "transpile:verbose": "sucrase ./src -d ./resources/core --transforms typescript",
    "[-] Webview": "",
    "webview:build": "pnpm vite build ./webview",
    "webview:dev": "pnpm vite dev ./webview"
  },
  "devDependencies": {
    "@altv/types-client": "^16.2.2",
    "@altv/types-natives": "^16.2.0",
    "@altv/types-server": "^16.2.1",
    "@altv/types-shared": "^16.2.1",
    "@altv/types-webview": "^16.2.1",
    "@types/node": "^20.12.7",
    "altv-pkg": "^2.7.5",
    "autoprefixer": "^10.4.19",
    "fast-glob": "^3.3.2",
    "fkill": "^9.0.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "shx": "^0.3.4",
    "sucrase": "^3.35.0",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.2"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vue": "^3.4.23",
    "vue-tsc": "^2.0.13"
  }
}
